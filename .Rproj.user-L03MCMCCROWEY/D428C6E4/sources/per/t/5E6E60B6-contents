library(keyring)
library(magrittr)
library(withr)

testthatsetup <- function()
  {
    #AQSusernamefortest <- Sys.setenv("RAQSAPIUSERNAME" = "mccrowey.clinton@epa.gov")
    AQSusername <- "mccrowey.clinton@epa.gov"
    AQSkey <- key_get(service = "AQSDatamartAPI",
                       username = "mccrowey.clinton@epa.gov")

    #withr::defer(expr = c(AQSusernamefortest, AQSkeyfortest))
    return(list(AQSusername = AQSusername , AQSkey = AQSkey))
    }
