{
  "name": "RAQSAPI Dev Container",
  "image": "rocker/geospatial:latest",
  "workspaceFolder": "/workspaces/RAQSAPI",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/RAQSAPI,type=bind,consistency=cached",
  "features": {},
  "forwardPorts": [],
  "postCreateCommand": "R --version",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "REditorSupport.r",
				"RDebugger.r-debugger",
				"quarto.quarto"]
    }
  }
}
